{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "noogle",
      "camelcase": "Noogle",
      "title": "Noogle",
      "scope": "source.noogle",
      "file-types": [
        "noog"
      ],
      "highlights": [
        "queries/highlights.scm"
      ],
      "injection-regex": "^noogle$",
      "class-name": "TreeSitterNoogle"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Noogle grammar for tree-sitter",
    "authors": [
      {
        "name": "Ivan",
        "email": "ivannovitskii@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-noogle"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}